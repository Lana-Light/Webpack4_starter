{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "webpack4 starter for pug and scss projects",
  "main": "index.js",
  "browserslist": [
    "> 1%",
    "ie 11",
    "not dead"
  ],
  "scripts": {
    "grid": "node smart-grid.config.js",
    "start": "webpack-dev-server --open --mode development --hot",
    "watch": "webpack --watch",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --open --config webpack.config.js --mode development",
    "pretty": "prettier --write ./src/{js7/**/*.js,scss/**/*.scss}",
    "test": "npm test"
  },
  "keywords": [
    "webpack",
    "pug",
    "scss",
    "babel"
  ],
  "author": "Lana-Light",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "advpng-bin": "^3.0.0",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.6.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.5.6",
    "file-loader": "^3.0.1",
    "group-css-media-queries-loader": "^3.0.2",
    "html-webpack-multi-build-plugin": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-advpng": "^5.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webpack": "^4.1.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "smart-grid": "^2.1.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5"
  }
}
